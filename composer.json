{
    "name": "toastnz/mercury",
    "type": "silverstripe-vendormodule",
    "description": "Toast NZ SilverStripe Installer",
    "require": {
        "php": ">=8.1",
        "silverstripe/recipe-cms": "4.11.0@stable",
        "undefinedoffset/silverstripe-nocaptcha": "^2.3",
        "symbiote/silverstripe-gridfieldextensions": "^3.4",
        "jonom/focuspoint": "^4.0",
        "drewm/mailchimp-api": "^2.5",
        "silverstripe/googlesitemaps": "^2.2",
        "silverstripe/taxonomy": "^2.4",
        "silverstripe/redirectedurls": "^2.1",
        "betterbrief/silverstripe-googlemapfield": "^2.2",
        "bummzack/sortablefile": "^2.1",
        "symbiote/silverstripe-queuedjobs": "^4.10",
        "silverstripe/userforms": "^5.13",
        "silverstripe/tagfield": "^2.9",
        "stevie-mayhew/silverstripe-svg": "^2.2",
        "undefinedoffset/sortablegridfield": "^2.1",
        "silverstripe/blog": "^3.10",
        "axllent/silverstripe-form-fields": "^1.6",
        "permanentinc/found": "^1.3",
        "permanentinc/type": "dev-master",
        "permanentinc/beautiful": "^0.4.1",
        "heyday/silverstripe-menumanager": "^3.2",
        "toastnz/duplicator": "^1.0",
        "toastnz/webp": "^1.3",
        "toastnz/favicon": "dev-main",
        "toastnz/linkable": "^2",
        "plasticstudio/iconfield": "^1.1",
        "unclecheese/silverstripe-image-optionset": "^2.0",
        "toastnz/blocks": "^3.0"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
